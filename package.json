{
  "name": "api-template",
  "version": "1.0.0",
  "description": "api template",
  "author": "Tommy Duong",
  "license": "ISC",
  "scripts": {
    "coverage": "tsc -p tsconfig.json",
    "coverage:serve": "npm run coverage && npm run serve:lcov",
    "dev": "nodemon",
    "down": "docker-compose down",
    "env": "export $(cat .env | xargs)",
    "knex": "knex --knexfile ./src/config/Knexfile.ts",
    "lint": "tslint -p tslint.json",
    "serve:lcov": "serve -s coverage/lcov-report",
    "start": "ts-node src/",
    "test": "npm run test:base --watch",
    "test:base": "jest --config jest.config.js -i",
    "test:ci": "CI=true npm run coverage && cat ./coverage/lcov.info | codecov",
    "up": "npm run env && docker-compose up",
    "up:test": "npm run env && docker-compose -f docker-compose-test.yaml up --build",
    "up:b": "npm run up --build",
    "up:d": "npm run up -d",
    "verify": "tsc"
  },
  "dependencies": {
    "@juliancoleman/named-error": "^1.0.0",
    "@types/knex": "^0.15.2",
    "bluebird": "^3.5.3",
    "celebrate": "^9.1.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "firebase-admin": "^7.0.0",
    "joi": "^14.3.0",
    "knex": "0.15.2",
    "objection": "^1.6.3",
    "pg": "^7.9.0",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.16.1",
    "underscore.string": "^3.3.5"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/joi": "^14.3.2",
    "@types/node": "^11.11.3",
    "@types/pg": "^7.4.13",
    "@types/ramda": "^0.26.3",
    "@types/supertest": "^2.0.7",
    "codecov": "^3.2.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-spec-reporter": "^1.0.5",
    "nodemon": "^1.18.10",
    "serve": "^10.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
